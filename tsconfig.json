{
  "include": [
      "src/**/*.ts"
  ],
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@lib/*": ["lib/*"],
    },
    "module": "commonjs",
    "target": "es6",
    "outDir": "dist",
    "rootDir": "./src",
    "sourceMap": true,
    "strict": false,
    "esModuleInterop": true
  }
}